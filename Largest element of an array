#include<stdio.h>
void main() {
    int n;
    printf("Enter the number of the elements in the array: ");
    scanf("%d", &n);
    int arr[n];
    for (int i = 0; i < n; i++) {
    printf("Enter the %dth elements of the array: ",i);
        scanf("%d", &arr[i]);
    }
    
    int largest = arr[0];
    for (int i = 1; i < n; i++) {
        if (arr[i] > largest) {
            largest = arr[i];
        }
    }
    printf("The largest element in the array is: %d\n", largest);
}
