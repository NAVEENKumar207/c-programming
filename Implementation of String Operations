! . Find the Length of a String.
  ★ Note: Without Using library functions[strlen].
**************************************************************************************
Coding:
#include <stdio.h>
void main()
 {
    printf("☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼");
    printf("\n Program By ARSHIYA I I-year B.Tech CSBS");
    printf("\n ☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼☼");
    char str[100]; 
    int length = 0; 
    printf("\n Enter a string: ");
    scanf("%s", str); 
    while (str[length] != '\0')
    { 
        length++;
    }
   printf("Length of the string: %d\n", length);
}
