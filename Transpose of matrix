#include<stdio.h>
void main()
{
    int r,c;
    printf("Enter the number of rows : ");
    scanf("%d",&r);
    printf("Enter the number of coloumns : ");
    scanf("%d",&c);
    int matrix[r][c];
    int transpose[c][r];
    for (int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
                printf("Enter the element of matrix[%d][%d]:",i,j);
            scanf("%d",&matrix[i][j]);
        }
    }
    printf("\nDisplaying the matrix\n");
     for (int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            printf("%d ",matrix[i][j]);
            transpose[i][j]=matrix[i][j];
        }
        printf("\n");
    }
  printf("\nTransposed matrix\n");
  for (int i=0;i<c;i++)
    {
        for(int j=0;j<r;j++)
        {
            printf("%d ",transpose[j][i]);
        }
        printf("\n");
    }
    
}
